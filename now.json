{
  "version": 2,
  "name": "image-uploader-now",
  "regions": ["hnd1"],
  "public": false,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "src/server/routes/*.js", "use": "@now/node-server" }
  ],
  "github": { "silent": true },
  "routes": [
    { "src": "/api/upload", "dest": "/src/server/routes/upload.js" },
    { "src": "/api/oauth", "dest": "/src/server/routes/oauth.js" }
  ]
}
